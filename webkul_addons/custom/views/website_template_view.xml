        <template id="mp_seller_signup_registration_question" inherit_id="odoo_marketplace.mp_seller_signup"
                  name="Seller Sign up login Registration Questions">
            <xpath expr="//div[@id='mp_seller_details']" position="after">
                <input type="checkbox" name="register_questions" id="register_questions" />
                <label for="register_questions" class="form-group control-label">&#160; Registration question for the seller.</label>
                <label class="control-label" for="company">Company Name</label>
                <input class="form-control" type="text" placeholder="Enter company Name...." name="company" id="company"/>
                <label class="control-label" for="vat_no">VAT number</label>
                <input class="form-control" type="text" placeholder="Enter Your VAT Number ...." name="vat_no" id="vat_no"/>
                <label class="control-label" for="company_web">Company website</label>
                <input class="form-control" type="text" placeholder="Enter Your Company Website ...." name="company_web" id="company_web"/>

            </xpath>
        </template>
    </data>
