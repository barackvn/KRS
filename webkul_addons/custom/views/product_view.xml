<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="wk_seller_product_template_form_view_inherit" model="ir.ui.view">
            <field name="name">seller.product.template.common.form.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="odoo_marketplace.wk_seller_product_template_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='hs_code']" position="replace">
                </xpath>
                <xpath expr="//field[@name='exclusively_id']" position="replace">
                </xpath>
                <xpath expr="//field[@name='default_code']" position="attributes">
                    <attribute name="readonly">1</attribute>
                </xpath>
                <xpath expr="//field[@name='lst_price']" position="replace">
                    <separator string="Prices for your buyer"/>
                    <field name="lst_price" string="Sellers price"/>
                    <field name="purchase_price_carton_excl_vat" />
                    <field name="purchase_price_unit_excl_vat" />
                    <field name="adv_sale_price_excl_vat" />
                    <field name="krs_margin" />
                    <field name="purchase_price_carton_incl_vat" />
                    <field name="purchase_price_unit_incl_vat" />
                    <field name="adv_sale_price_incl_vat" />
                    <field name="product_minimum_qty" />

                </xpath>
                <xpath expr="//field[@name='marketplace_seller_id']" position="after">
                    <field name="per_product_ff_charge"/>
                    <field name="hs_code" string="Product classification code / HS code"/>
                    <field name="unit_article_no"/>
                    <field name="carton_article_no"/>
                    <field name="unit_barcode"/>
                    <field name="carton_barcode"/>
                    <field name="exclusively_id"/>
                    <field name="qty_single_unit_per_cartn"/>
                    <field name="conservation"/>
                </xpath>
                <xpath expr="//field[@name='barcode']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>

                <xpath expr="//group[@name='pricelists']" position="before">
                    <group>
                        <group>
                            <separator string="Description for your buyers"/>
                            <field name="ingredients"/>
                            <field name="product_short_discrp"/>
                            <field name="product_short_discrp"/>
                            <separator string="Nutritional information"/>
                            <field name="nutriscore"/>
                            <field name="energy_kg"/>
                            <field name="energy_kcal"/>
                            <field name="fat"/>
                            <field name="saturated_fat"/>
                            <field name="mono_unsaturated_fats"/>
                            <field name="poly_unsaturated_fats"/>
                            <field name="carbohydrates"/>
                            <field name="sugar"/>
                            <field name="protein"/>
                            <field name="salt"/>
                            <field name="cal_nutri_percent"/>
<!--                            <field name="product_place"/>-->
<!--                            <field name="origin_type"/>-->
<!--                            <field name="method_capture"/>-->
                            <separator string="Instructions"/>
                            <field name="instruction"/>
                            <field name="storage_instructions"/>
                            <field name="serving_sug"/>
                            <field name="prepare_instr"/>
                            <separator string="Origins"/>
                            <field name="place_birth"/>
                            <field name="place_assembly"/>
                            <field name="place_entry"/>
                            <field name="place_milking"/>
                            <field name="place_fattening"/>
                            <field name="place_bottling"/>
                            <field name="place_operation"/>
                            <field name="place_slaughter"/>
                            <field name="place_product_grown"/>
                            <separator string="Fishing origins"/>
                            <field name="fish_zone_id"/>
                            <field name="fishing_id"/>

                        </group>
                        <group>
                            <separator string="Allergens"/>
                            <field name="gluten_contain_grains"/>
                            <field name="milk_base_product"/>
                            <field name="eggs_base_product"/>
                            <field name="peanuts_base_product"/>
                            <field name="nuts_base_product"/>
                            <field name="soy_base_product"/>
                            <field name="mustard_base_product"/>
                            <field name="lupine_base_product"/>
                            <field name="sesame_base_product"/>
                            <field name="fish_base_product"/>
                            <field name="molluscs_base_product"/>
                            <field name="shellfish_base_product"/>
                            <field name="sulphites_base_product"/>
                            <separator string="Food claims"/>
                            <field name="allergen_free"/>
                            <field name="baby"/>
                            <field name="dietary"/>
                            <field name="fair_trade"/>
                            <field name="gmo_free"/>
                            <field name="halal"/>
                            <field name="kosher"/>
                            <field name="lactose_free"/>
                            <field name="low_cholesterol"/>
                            <field name="low_sodium"/>
                            <field name="organic"/>
                            <field name="palm_oil_free"/>
                            <field name="paraben_free"/>
                            <field name="rich_omega"/>
                            <field name="stevia"/>
                            <field name="sugar_free"/>
                            <field name="vegan"/>
                            <field name="vegon_ok"/>
                            <field name="vegetarian"/>
                            <field name="without_preservative"/>
                            <separator string="Accreditations &amp; certificates"/>
                            <field name="local_accredit_package"/>
                            <field name="accredit_package_id"/>
                            <field name="regulatory_id"/>
                            <field name="nutrition_id"/>
                            <field name="diet_id"/>
                            <separator string="BIO origins"/>
                            <field name="location_origin"/>
                            <field name="country_origin_id"/>
                            <field name="region_country"/>




<!--                            <field name="area_capture"/>-->

<!--                            <field name="allergens"/>-->
                        </group>
                    </group>
                </xpath>
                <xpath expr="//group[@name='operation']" position="replace">
                    <group string="Operations" name="operation">
                        <label for="sale_delay"/>
                        <div>
                            <field name="sale_ok" invisible="1"/>
                            <field name="sale_delay" attrs="{'readonly':[('sale_ok','=',False)]}" class="oe_inline"
                                   style="vertical-align:baseline"/>
                            days
                        </div>
                        <label for="manufacture_days"/>
                        <div>
                            <field name="manufacture_days"  class="oe_inline"
                                   style="vertical-align:baseline"/>
                            days
                        </div>
                        <label for="transport_days"/>
                        <div>
                            <field name="transport_days"  class="oe_inline"
                                   style="vertical-align:baseline"/>
                            days
                        </div>
                        <label for="fulfilment_days"/>
                        <div>
                            <field name="fulfilment_days"  class="oe_inline"
                                                               style="vertical-align:baseline"/>
                            days
                        </div>
                    </group>
                </xpath>
                <xpath expr="//field[@name='qty']" position="after">
                    <label for="product_alert_day"/>
                    <div>
                        <field name="product_alert_day"  class="oe_inline"
                                                           style="vertical-align:baseline"/>
                        days
                    </div>
                    <label for="product_removal_days"/>
                    <div>
                        <field name="product_removal_days"  class="oe_inline"
                                                           style="vertical-align:baseline"/>
                        days
                    </div>
                    <label for="min_shelf_delivery"/>
                    <div>
                        <field name="min_shelf_delivery"  class="oe_inline"
                                                           style="vertical-align:baseline"/>
                        days
                    </div>
                    <label for="product_use_days"/>
                    <div>
                        <field name="product_use_days"  class="oe_inline"
                                                           style="vertical-align:baseline"/>
                        days
                    </div>
                </xpath>
                <xpath expr="//field[@name='weight']" position="after">
                    <field name="length_unit"/>
                    <field name="width_unit"/>
                    <field name="height_unit"/>
                    <field name="volume_unit"/>
                    <field name="length_carton"/>
                    <field name="width_carton"/>
                    <field name="height_carton"/>
                    <field name="volume_carton"/>
                    <field name="net_content_per_unit"/>
                    <field name="drained_weight_per_unit"/>
                    <field name="net_weight_per_unit"/>
                    <field name="gross_weight_per_unit"/>
                    <field name="net_content_per_carton"/>
                    <field name="drained_weight_per_carton"/>
                    <field name="net_weight_per_carton"/>
                    <field name="gross_weight_per_carton"/>
                    <field name="pallet_quantity"/>
                    <field name="kind_pallet"/>
                    <field name="total_pallet_height"/>
                </xpath>
                <xpath expr="//field[@name='volume']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//field[@name='weight']" position="attributes">
                    <attribute name="invisible">1</attribute>
                </xpath>
                <xpath expr="//notebook/page/separator" position="before">
                    <field name="attachment_ids" widget="many2many_binary" string="Attach a file"/>
                </xpath>

            </field>
        </record>

