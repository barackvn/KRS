            <field name="icon" type="base64" file="social_media_tabs/static/src/images/custom.jpg"/>
            <field name="media_type">custom</field>
            <field name="tab_sequence">8</field>
            <field name="color">#9E9E9E</field>
            <field name="title">Contact Us</field>
            <field name="custom_tab_name">Custom Tab</field>
            <field name="logo" type="base64" file="social_media_tabs/static/src/images/shares.png"/>
            <field name="custom_html"><![CDATA[
                <p><font style="font-size: 36px;" class="text-epsilon">CUSTOM TAB</font></p><p><font style="color: rgb(255, 0, 255); font-size: 36px;"> </font><font style="color: rgb(57, 123, 33); font-size: 36px;">This is a test custom tab</font></p><p>&nbsp;&nbsp;&nbsp;&nbsp;<font style="color: rgb(57, 123, 33); font-size: 36px;">&nbsp;</font>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<font style="color: rgb(57, 123, 33); font-size: 36px;">&nbsp;</font>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<font style="color: rgb(57, 123, 33); font-size: 36px;">&nbsp;</font>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<font style="color: rgb(57, 123, 33); font-size: 36px;">&nbsp;</font>&nbsp;&nbsp;&nbsp;&nbsp;<img src="/website/static/src/img/library/sweet.jpg" class="pull-left" style="width: 100%;" title=""><font style="color: rgb(57, 123, 33); font-size: 36px;"><br></font><br></p>]]></field>
        </record>

        <record id="social_res_config" model="social.tab.config.settings">
            <field name="tab_ids" eval="[(6, 0, [ref('social_media_tabs.facebook_social_tab'),ref('social_media_tabs.tumblr_social_tab'),ref('social_media_tabs.twitter_social_tab'),ref('social_media_tabs.youtube_social_tab'),ref('social_media_tabs.instagram_social_tab'),ref('social_media_tabs.pinterest_social_tab'),ref('social_media_tabs.flickr_social_tab')])]"/>
        </record>

