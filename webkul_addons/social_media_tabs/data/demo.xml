        <record id="facebook_social_tab" model="social.media.tabs">
            <field name="icon" type="base64" file="social_media_tabs/static/src/images/fb.png"/>
            <field name="media_type">social_tab</field>
            <field name="social_tab">facebook</field>
            <field name="tab_sequence">1</field>
            <field name="color">#283593</field>
            <field name="title">Facebook</field>
            <field name="folow">Follow on facebook</field>
